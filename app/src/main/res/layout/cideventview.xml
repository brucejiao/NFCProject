<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffffff" >

    <TextView
        android:id="@+id/tv_intelbrass_cid_count"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="" />

    <Button
        android:id="@+id/bt_intelbrass_cid_ret"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bt_intelbrass_cid_start"
        android:layout_alignBottom="@+id/bt_intelbrass_cid_start"
        android:layout_marginLeft="41dp"
        android:layout_toRightOf="@+id/bt_intelbrass_cid_start"
        android:text="@string/bt_return" />

    <Button
        android:id="@+id/bt_intelbrass_cid_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/tv_intelbrass_cid_count"
        android:layout_marginLeft="62dp"
        android:layout_marginTop="20dp"
        android:text="@string/intelbrass_cid_start" />

</RelativeLayout>
