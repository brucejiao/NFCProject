    </style>

    <style name="Theme_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="spinner_open_icon">
        <item name="android:layout_width">16dp</item>
        <item name="android:layout_height">16dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:src">@drawable/mm_submenu</item>
    </style>


    <style name="spinner_styles">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@null</item>
        <item name="android:drawSelectorOnTop">true</item>

    </style>

    <style name="loading_small" parent="@android:style/Widget.ProgressBar.Small">
        <item name="android:indeterminateDrawable">@drawable/loading</item>
        <item name="android:layout_width">21dip</item>
        <item name="android:layout_height">21dip</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>
</resources>
